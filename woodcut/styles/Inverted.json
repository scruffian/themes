{
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "header-footer-only",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Header and Footer Only"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [
						"#000",
						"#000",
						"#000",
						"#fff",
						"#fff",
						"#fff"
					],
					"name": "Default filter",
					"slug": "default-filter"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#000000",
					"name": "Background",
					"slug": "background"
				}
			]
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Crimson Text",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/CrimsonText-Regular.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Crimson Text",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/CrimsonText-SemiBold.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Crimson Text",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/CrimsonText-Bold.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Crimson Text",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/CrimsonText-Italic.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Crimson Text",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/CrimsonText-SemiBoldItalic.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Crimson Text",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/CrimsonText-BoldItalic.ttf"
							]
						}
					],
					"fontFamily": "\"Crimson Text\", serif",
					"name": "Crimson Text",
					"slug": "crimson-text"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Roboto Mono",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/RobotoMono-VariableFont_wght.ttf"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Roboto Mono",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/RobotoMono-Italic-VariableFont_wght.ttf"
							]
						}
					],
					"fontFamily": "\"Roboto Mono\", sans-serif",
					"name": "Roboto Mono",
					"slug": "roboto-mono"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1em",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "3rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "4rem",
					"slug": "x-large"
				},
				{
					"name": "Ginormous",
					"size": "5rem",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/code": {
				"border": {
					"color": {
						"ref": "styles.color.text"
					},
					"radius": "0px",
					"style": "solid",
					"width": "10px"
				},
				"spacing": {
					"padding": "var(--wp--style--block-gap)"
				}
			},
			"core/cover": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				},
				"spacing": {
					"padding": "15vh"
				}
			},
			"core/image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				}
			},
			"core/post-author": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				}
			},
			"core/post-date": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/post-featured-image": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--default-filter)"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"color": {
						"ref": "styles.color.text"
					},
					"style": "solid",
					"width": "10px 0"
				},
				"spacing": {
					"padding": "var(--wp--style--block-gap)"
				},
				"typography": {
					"fontStyle": "italic"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--custom--color--primary)",
					"style": "solid",
					"width": "0 0 0 10px"
				},
				"spacing": {
					"margin": "var(--wp--style--block-gap)",
					"padding": {
						"left": "var(--wp--style--block-gap)"
					}
				},
				"typography": {
					"fontStyle": "italic"
				}
			},
			"core/search": {
				"border": {
					"color": {
						"ref": "styles.color.text"
					},
					"radius": "0px",
					"style": "solid",
					"width": "5px"
				},
				"elements": {
					"button": {
						"spacing": {
							"margin": "0px"
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": {
						"ref": "styles.color.text"
					},
					"radius": "0px",
					"style": "solid",
					"width": "3px"
				},
				"spacing": {
					"margin": {
						"bottom": "5vw",
						"top": "5vw"
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "900"
				}
			}
		},
		"color": {
			"background": "var:preset|color|background",
			"text": "var:preset|color|foreground"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0px"
				},
				"color": {
					"background": {
						"ref": "styles.color.text"
					},
					"text": {
						"ref": "styles.color.background"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "900"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "900"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "900"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "900"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "900"
				}
			},
			"link": {
				"color": {
					"text": {
						"ref": "styles.color.text"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "1em"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--crimson-text)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.2"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}
