					"slug": "primary"
				},
				{
					"color": "#006ba1",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "rgba(30,30,30,0.8)",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "rgba(255,250,245,1)",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#334455",
					"name": "Tertiary",
					"slug": "tertiary"
