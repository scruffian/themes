	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customGradient": true,
			"gradients": [
				{
					"gradient": "conic-gradient(at 80vw 80vh, var(--wp--preset--color--primary) 45deg,transparent 0,transparent 315deg, var(--wp--preset--color--primary) 0) fixed",
					"name": "Triangle",
					"slug": "triangle"
				},
				{
					"gradient": "conic-gradient(at 0% 50%, transparent 45deg, var(--wp--preset--color--primary) 45deg, var(--wp--preset--color--primary) 135deg, transparent 135deg )",
					"name": "Triangle",
					"slug": "triangle-prev"
				},
				{
					"gradient": "conic-gradient(at 100% 50%, transparent 225deg, var(--wp--preset--color--primary) 225deg, var(--wp--preset--color--primary) 315deg, transparent 315deg )",
					"name": "Triangle",
					"slug": "triangle-next"
				},
				{
					"gradient": "linear-gradient(135deg, transparent 0%, transparent 10%, var(--wp--preset--color--foreground) 10%, var(--wp--preset--color--foreground) 90%, transparent 90%)",
					"name": "Triangle",
					"slug": "triangle-button"
				},
				{
					"gradient": "linear-gradient(135deg, transparent 0%, transparent 10%, var(--wp--preset--color--tertiary) 10%, var(--wp--preset--color--tertiary) 90%, transparent 90%)",
					"name": "Triangle",
					"slug": "triangle-button-hover"
				}
			],
			"palette": [
				{
					"color": "rgba(15, 58, 115,0.1)",
