	"styles": {
		"blocks": {
			"core/code": {
				"border": {
					"color": "var(--wp--preset--color--foreground)",
					"radius": "0.25rem",
					"style": "solid",
					"width": "2px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontFamily": "monospace"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/gallery": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--70)"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-navigation-link": {
				"color": {
					"gradient": "var(--wp--preset--gradient--triangle-prev)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"left": "1ch"
					},
					"padding": {
						"left": "1ch"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--triangulate)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/post-terms": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"gradient": "var(--wp--preset--gradient--triangle-button-hover)",
								"text": "var(--wp--preset--color--background)"
							}
						},
						"color": {
							"gradient": "var(--wp--preset--gradient--triangle-button)",
							"text": "var(--wp--preset--color--background)"
						},
						"spacing": {
							"padding": {
								"bottom": "var(--wp--preset--spacing--40)",
								"left": "var(--wp--preset--spacing--50)",
								"right": "var(--wp--preset--spacing--50)",
								"top": "var(--wp--preset--spacing--40)"
							}
						},
						"typography": {
							"lineHeight": "3",
							"textDecoration": "none",
							"textTransform": "uppercase"
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--foreground)",
					"style": "solid",
					"width": "1px 0"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "italic"
				}
			},
			"core/query-pagination-next": {
				"color": {
					"gradient": "var(--wp--preset--gradient--triangle-next)"
				},
				"spacing": {
					"padding": {
						"left": "1ch",
						"right": "2ch"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--triangulate)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"textDecoration": "none"
				}
			},
			"core/query-pagination-previous": {
				"color": {
					"gradient": "var(--wp--preset--gradient--triangle-prev)"
				},
				"spacing": {
					"padding": {
						"left": "2ch",
						"right": "1ch"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--triangulate)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"textDecoration": "none"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "0",
					"textTransform": "none"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--foreground)",
					"style": "solid",
					"width": "0 0 0 1px"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontStyle": "normal"
				}
			},
			"core/search": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.6"
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"color": "transparent",
						"width": "30vw"
					},
					"right": {
						"color": "transparent",
						"width": "30vw"
					},
					"style": "solid",
					"top": {
						"color": "transparent",
						"width": "30vw"
					},
					"width": "30vw"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--tertiary)"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": 0
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"gradient": "var(--wp--preset--gradient--triangle)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				":active": {
					"color": {
						"gradient": "var(--wp--preset--gradient--triangle-button-hover)"
					}
				},
				":focus": {
					"color": {
						"gradient": "var(--wp--preset--gradient--triangle-button-hover)"
					}
				},
				":hover": {
					"color": {
						"gradient": "var(--wp--preset--gradient--triangle-button-hover)"
					}
				},
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "transparent",
					"gradient": "var(--wp--preset--gradient--triangle-button)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "2vw",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.5rem, calc(1.5rem + ((1vw - 0.48rem) * 0.4808)), 1.75rem)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.25rem, calc(1.25rem + ((1vw - 0.48rem) * 0.4808)), 1.5rem)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--triangulate)",
					"fontWeight": "400",
					"lineHeight": "1.125",
					"textTransform": "uppercase"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--tertiary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"bottom": "5vw",
				"left": "5vw",
				"right": "5vw",
				"top": "5vw"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json"
